    "workbox-webpack-plugin": "^5.1.4"
  },
  "scripts": {
    "start": "cross-env PORT=3006 node scripts/start.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "prod": "cross-env NODE_ENV=production PORT=3006 node scripts/start.js",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:local": "cross-env NODE_ENV=local jest --watch",
    "test:watch": "cross-env NODE_ENV=test jest --watch --verbose",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk jest --runInBand --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --notify --coverage",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier  \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "format": "npm run prettier -- --write",
    "validate-staged": "lint-staged",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "serve": "serve -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@types/enzyme": "^3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.4",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.5",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.21",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.0.11",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "cross-env": "^7.0.2",
    "cypress-testing-library": "^2.3.6",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.3.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "is-ci-cli": "^1.1.1",
    "lint-staged": "^8.1.5",
    "mobx-react-devtools": "^6.0.3",
    "prettier": "^1.17.0",
    "react-testing-library": "^6.0.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.2.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate-staged"
    }
  },
  "proxy": "http://localhost:3000"
}
